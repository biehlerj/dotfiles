[commit]
  ; gpgsign = true
[core]
  pager = delta
[interactive]
  diffFilter = delta --color-only
[delta]
  side-by-side = true
  line-numbers-left-format = ""
  line-numbers-right-format = "| "
  syntax-theme = Dracula
[mergetool "nvim"]
  cmd = nvim -f -c \"Gdiff\" \"$MERGED\"
[pull]
  rebase = true
[init]
	defaultBranch = master
