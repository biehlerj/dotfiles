[user]
  name = Jacob Biehler
  email = jacob@biehlerj.xyz
[gpg]
  ; program = gpg2
[commit]
  ; gpgsign = true
[core]
  pager = delta
  editor = code --wait
[interactive]
  diffFilter = delta --color-only
[delta]
  side-by-side = true
  line-numbers-left-format = ""
  line-numbers-right-format = "| "
  syntax-theme = Dracula
[merge]
  tool = vscode
[mergetool "vscode"]
  cmd = code --wait $MERGED
[pull]
  rebase = true
[init]
  defaultBranch = master
